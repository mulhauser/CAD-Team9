<?xml version="1.0" encoding="UTF-8"?>
<project name="cad-team9" default="all">
  
  
  <!--<property file="cad-team9.properties"/>-->
  <!-- Uncomment the following property if no tests compilation is needed -->
  <!-- 
  <property name="skip.tests" value="true"/>
   -->
  
  <!-- Compiler options -->
  
  <property name="compiler.debug" value="on"/>
  <property name="compiler.generate.no.warnings" value="off"/>
  <property name="compiler.args" value=""/>
  <property name="compiler.max.memory" value="700m"/>
  <patternset id="ignored.files">
    <exclude name="**/*.hprof/**"/>
    <exclude name="**/*.pyc/**"/>
    <exclude name="**/*.pyo/**"/>
    <exclude name="**/*.rbc/**"/>
    <exclude name="**/*.yarb/**"/>
    <exclude name="**/*~/**"/>
    <exclude name="**/.DS_Store/**"/>
    <exclude name="**/.git/**"/>
    <exclude name="**/.hg/**"/>
    <exclude name="**/.svn/**"/>
    <exclude name="**/CVS/**"/>
    <exclude name="**/__pycache__/**"/>
    <exclude name="**/_svn/**"/>
    <exclude name="**/vssver.scc/**"/>
    <exclude name="**/vssver2.scc/**"/>
  </patternset>
  <patternset id="library.patterns">
    <include name="*.war"/>
    <include name="*.swc"/>
    <include name="*.zip"/>
    <include name="*.ear"/>
    <include name="*.egg"/>
    <include name="*.ane"/>
    <include name="*.jar"/>
  </patternset>
  <patternset id="compiler.resources">
    <exclude name="**/?*.java"/>
    <exclude name="**/?*.form"/>
    <exclude name="**/?*.class"/>
    <exclude name="**/?*.groovy"/>
    <exclude name="**/?*.scala"/>
    <exclude name="**/?*.flex"/>
    <exclude name="**/?*.kt"/>
    <exclude name="**/?*.clj"/>
    <exclude name="**/?*.aj"/>
  </patternset>
  
  <property name="project.jdk.home" value=""/>
  <property name="project.jdk.bin" value=""/>
  <property name="project.jdk.classpath" value=""/>
  
  <!-- Modules -->
  
  
  <!-- Module CAD-Team9 -->
  
  <dirname property="module.cad-team9.basedir" file="${ant.file}"/>
  
  
  <property name="module.jdk.home.cad-team9" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.cad-team9" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.cad-team9" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.cad-team9" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="cad-team9.output.dir" value="${module.cad-team9.basedir}/out/production/CAD-Team9"/>
  <property name="cad-team9.testoutput.dir" value="${module.cad-team9.basedir}/out/test/CAD-Team9"/>
  
  <path id="cad-team9.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="cad-team9.module.production.classpath"/>
  
  <path id="cad-team9.runtime.production.module.classpath">
    <pathelement location="${cad-team9.output.dir}"/>
  </path>
  
  <path id="cad-team9.module.classpath">
    <pathelement location="${cad-team9.output.dir}"/>
  </path>
  
  <path id="cad-team9.runtime.module.classpath">
    <pathelement location="${cad-team9.testoutput.dir}"/>
    <pathelement location="${cad-team9.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.cad-team9">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.cad-team9">
    <patternset refid="excluded.from.module.cad-team9"/>
  </patternset>
  
  
  <target name="compile.module.cad-team9" depends="compile.module.cad-team9.production,compile.module.cad-team9.tests" description="Compile module CAD-Team9"/>
  
  <target name="compile.module.cad-team9.production" description="Compile module CAD-Team9; production classes"/>
  
  <target name="compile.module.cad-team9.tests" depends="compile.module.cad-team9.production" description="compile module CAD-Team9; test classes" unless="skip.tests"/>
  
  <target name="clean.module.cad-team9" description="cleanup module">
    <delete dir="${cad-team9.output.dir}"/>
    <delete dir="${cad-team9.testoutput.dir}"/>
  </target>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.cad-team9" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.cad-team9" description="build all modules"/>
  
  <target name="all" depends="build.modules" description="build all"/>
</project>